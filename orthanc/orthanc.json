{
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": true,
  "RegisteredUsers": {
    "demo": "demo"
  },
  "HttpPort": 8042,
  "Plugins": [
    "/usr/local/share/orthanc/plugins/libOrthancPython.so",
    "/usr/local/share/orthanc/plugins/libOrthancDicomWeb.so",
    "/usr/local/share/orthanc/plugins/libOrthancPostgreSQLIndex.so",
    "/usr/local/share/orthanc/plugins/libOrthancPostgreSQLStorage.so"
  ],
  "PostgreSQL": {
    "EnableIndex": true,
    "EnableStorage": true,
    "Host": "postgres",
    "Port": 5432,
    "Database": "orthanc",
    "Username": "postgres",
    "Password": "pgpassword",
    "Lock": false,
    "TransactionMode": "ReadCommitted",
    "IndexConnectionsCount": 5,
    "UseDynamicConnectionPool": true
  },
  "PythonScript": "/src/python/main.py",
  "PythonVerbose": true,
  "HttpVerbose": true,
  "DicomModalities": {
    "TEST": {
      "AET": "ORTHANC",
      "Port": 4343,
      "Host": "host.docker.internal",
      "Manufacturer": "Generic"
    }
  },
  "StorageDirectory": "/var/lib/orthanc/db",
  "StableAge": 10,
  "UserContentType": {
    "resultsJson": [1025, "application/json"],
    "resultsCsvFull": [1026, "text/csv"],
    "resultsCsvTrunc": [1027, "text/csv"],
    "coronalPng": [1028, "image/png"]
  }
}
