{
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": true,
  "RegisteredUsers": {
    "demo": "demo"
  },
  "HttpPort": 8042,
  "Plugins": [
    "/usr/local/share/orthanc/plugins/libOrthancPython.so",
    "/usr/local/share/orthanc/plugins/libOrthancOHIF.so",
    "/usr/local/share/orthanc/plugins/libOrthancDicomWeb.so",
    "/usr/local/share/orthanc/plugins/libOrthancPostgreSQLIndex.so",
    "/usr/local/share/orthanc/plugins/libOrthancPostgreSQLStorage.so"
  ],
  "PostgreSQL": {
    "EnableIndex": true,
    "EnableStorage": true,
    "Host": "postgres",
    "Port": 5432,
    "Database": "orthanc",
    "Username": "postgres",
    "Password": "pgpassword",
    "Lock": false,
    "TransactionMode": "ReadCommitted",
    "IndexConnectionsCount": 5,
    "UseDynamicConnectionPool": true
  },
  "OHIF": {
    "DataSource": "dicom-web"
  },
  "PythonScript": "/src/python/main.py",
  "PythonVerbose": true,
  "HttpVerbose": true,
  "DicomCheckCalledAeTitle": false,
  "DicomAlwaysAllowEcho": true,
  "DicomAlwaysAllowStore": true,
  "DicomAlwaysAllowFind": true,
  "DicomAlwaysAllowMove": true,
  "DicomServerEnabled": true,
  "DicomModalities": {
    "SLICER": ["SLICER", "localhost", 11113]
  },
  "StableAge": 10
}
